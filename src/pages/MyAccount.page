                                        <p>Your current details are listed below. Edit to keep current.</p>
                                     </div>
                                </div>
                                
                                <apex:messages id="errorMsg"/>
                                
                                <apex:outputPanel id="idContactInfo" layout="block" styleClass="slds-m-top--medium">
