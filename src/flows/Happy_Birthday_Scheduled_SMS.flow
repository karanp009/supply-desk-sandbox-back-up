                               TEXT(MONTH({!myVariable_current.Date_of_Birth__c})) &amp; &quot;-&quot; &amp; 
                               TEXT(DAY({!myVariable_current.Date_of_Birth__c}))
                            ) &gt; TODAY(),
 
    /* Then schedule Birthday wish for this year 09:00 GMT = 9:00 MDT */
    DATETIMEVALUE( 
                                 TEXT(YEAR(TODAY())) &amp; &quot;-&quot; &amp; 
                                 TEXT(MONTH({!myVariable_current.Date_of_Birth__c})) &amp; &quot;-&quot; &amp; 
                                 TEXT(DAY({!myVariable_current.Date_of_Birth__c})) &amp; &quot; 09:00:00&quot; 
                               ),
 
    /* Else Add 1 Year and wish them Happy Birthday Next Year */
    DATETIMEVALUE( 
                                 TEXT(YEAR(TODAY())+1) &amp; &quot;-&quot; &amp; 
                                 TEXT(MONTH({!myVariable_current.Date_of_Birth__c})) &amp; &quot;-&quot; &amp; 
                                 TEXT(DAY({!myVariable_current.Date_of_Birth__c})) &amp; &quot; 09:00:00&quot; 
                               )
)</expression>
