        <fullName>TR1__Shortlist</fullName>
        <availability>online</availability>
        <description>Convert JobBoard Application to Application.</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Shortlist</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/33.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/33.0/apex.js&quot;)} 
var query=&quot;SELECT Id,Name,TR1__Applicant__c, TR1__Job__c, TR1__Job_Board_Source__c, TR1__Source__c, TR1__Status__c FROM TR1__Job_Board_Application__c Where Id = &apos;&quot; +&apos;{!TR1__Job_Board_Application__c.Id}&apos;+&quot;&apos;&quot;;
try{
     var selectedjobappln = sforce.connection.query(query);
     var selectedjobapplnrecords = selectedjobappln.getArray(&quot;records&quot;);
     if(selectedjobapplnrecords[0]==null){
         alert(&apos;No Data found with selected Records.&apos;);
         location.reload(true);
     }else{
         var isdelete = &quot;{!$Setup.TR1__TalentRoverProperties__c.TR1__Delete_JobBoard_Applications__c}&quot;;
         var newappln = new sforce.SObject(&quot;TR1__Application__c&quot;);
               newappln.TR1__Applicant__c=selectedjobapplnrecords[0].TR1__Applicant__c;
               newappln.TR1__Job__c=selectedjobapplnrecords[0].TR1__Job__c;
                           newappln.TR1__Job_Board_Source__c=selectedjobapplnrecords[0].TR1__Job_Board_Source__c;
             newappln.TR1__Status__c =&apos;Current&apos;;
               newappln.TR1__Source__c=selectedjobapplnrecords[0].TR1__Source__c;
            var result = sforce.connection.create([newappln]);
            if(result[0].success==&apos;true&apos;){
              if(isdelete ==&apos;true&apos;){
                var deleteappids =[];
                    deleteappids.push(&apos;{!TR1__Job_Board_Application__c.Id}&apos;);
                var result1 = sforce.connection.deleteIds(deleteappids);
                window.top.location=&apos;/&apos;+result[0].id;
                 }else{
                    var updatejobappln = new sforce.SObject(&quot;TR1__Job_Board_Application__c&quot;);
                        updatejobappln.Id=selectedjobapplnrecords[0].Id;
                        updatejobappln.TR1__Status__c =&apos;Converted&apos;;
                    var result2 = sforce.connection.update([updatejobappln]);
                    window.top.location=&apos;/&apos;+result[0].id;
                 }
            }else{alert(result[0].errors.message);}
      }
}catch(er){alert(er);}</url>
