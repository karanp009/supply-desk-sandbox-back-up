                    <td width="50%" valign="top">Date</td>
                    <td width="50%" valign="top"><apex:outputText value="{0,date,dd/MM/yyyy}"> <apex:param value="{!TR1__Closing_Report__c.TR1__Person_Placed__r.Barred_List_Date_Checked_Hidden__c}" /> </apex:outputText>
