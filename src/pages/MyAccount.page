                                                <div class="slds-modal__container Rt_Area">
                                                    <div class="slds-modal__header">
                                                        <h2 class="slds-text-heading--medium">Marketing Communications</h2>
                                                    </div>
                                                    <div class="slds-modal__content slds-p-horizontal--large">
                                                        <div id="modalMessage" style="overflow-x:auto;height:300px!important;">
                                                           <c:ArticleDetail artId="{!objCustomSettingForCandidate.MarketingCommunication__c}" order="0"/>
                                                        </div>
                                                    </div>
                                                    <div class="slds-modal__footer">
                                                        <div style="cursor:pointer" class="slds-button slds-button--neutral" onclick="closeMessageBanner('marketingID');">Close</div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="slds-modal-backdrop slds-modal-backdrop--open" id="modal2"></div>
                                        </div>
                                </div>
                            </apex:outputPanel>
                                
                                <apex:outputPanel id="idBankingInfo" layout="block" styleClass="slds-m-top--medium" rendered="{!!isClient}">
                                
                                    <div class="slds-grid slds-col--padded-large slds-wrap slds-grid--align-spread slds-m-top--medium slds-text-heading--medium">
                                        <div class="slds-col slds-large-size--1-of-1">
                                            <p></p>
                                        </div>
                                    </div>
                                
                                    <div class="slds-grid slds-wrap slds-grid--align-spread">
                                        <apex:repeat value="{!$ObjectType.Contact.Fieldsets.MyAccount2}" var="field">
                                            <div class="slds-col  slds-col--padded-large slds-size--1-of-1 slds-medium-size--3-of-6 slds-large-size--6-of-12"> 
                                                <label class="slds-form-element__static slds-m-top--medium" style="font-weight: bolder;" for="input{!field}">{!field.label}</label>
                                                
                                                <apex:outputPanel rendered="{!field.Type != 'boolean'}" layout="none">
                                                    <apex:inputField styleClass="slds-input" value="{!objContact[field]}" required="{!OR(field.Required,field.dbrequired)}" rendered="{!AND(field.Type != 'date',AND(field.Label!='First Name',field.Label!='Last Name'),isEditable)}" style="{!IF(field.type == 'textarea', 'height:100px;resize:none;', '')}   border-left:{!IF(OR(field.Required,field.dbrequired), '3px solid red', '' )}" />
                                                    <apex:outputPanel styleClass="slds-form-element__control" rendered="{!!isEditable}" layout="block" style="border:{!IF(OR(field.Type == 'boolean'), '','1px solid rgba(129, 129, 129, 0.19)')};">
                                                        <span class="slds-form-element__static slds-m-left--small">
                                                            <apex:outputField value="{!objContact[field]}"></apex:outputField>
                                                        </span>
                                                    </apex:outputPanel>  
                                                </apex:outputPanel>
                                            </div>
                                        </apex:repeat>
                                    </div>
                                </apex:outputPanel>
                                <br/><br/><br/>
                            
                                <!-- Start Popup window terms & condition -->
                                <div id="termsCond" style="display:none">
                                <div aria-hidden="false" role="dialog" class="slds-modal slds-fade-in-open" >
                                    <div class="slds-modal__container Rt_Area">
                                        <div class="slds-modal__header">
                                            <h2 class="slds-text-heading--medium">Terms of Use</h2>
                                        </div>
                                        <div class="slds-modal__content slds-p-horizontal--large">
                                            <div id="modalMessage" style="overflow-x:auto;height:300px!important;">
                                                <c:ArticleDetail artId="{!objCustomSettingForCandidate.PrivacyPolicy__c}" order="0"/>
                                            </div>
                                        </div>
                                        <div class="slds-modal__footer">
                                            <div style="cursor:pointer" class="slds-button slds-button--neutral" onclick="$('#termsCond').hide();">Close</div>
                                        </div>
                                    </div>
                                </div>
                                <div class="slds-modal-backdrop slds-modal-backdrop--open" id="modal2"></div>
                            </div>
                            
                                <!-- End Popup window for terms & Condition-->
                             
                                <apex:outputPanel id="idRelatedList" style="display:{!IF(isEditable, 'none', '')}">
                                    
                                    <div class="slds-tabs--scoped slds-col--padded-large">
                                        <ul class="slds-tabs--scoped__nav" role="tablist">
                                            <!--<li class="slds-tabs__item slds-text-heading--label slds-active" title="Education History" role="presentation" style="list-style: none;">
                                                <a href="#" role="tab" tabindex="0" aria-selected="true" aria-controls="tab-scoped-1">Education History</a>
                                            </li>
                                            <li class="slds-tabs__item slds-text-heading--label" title="Employment History" role="presentation">
                                                <a href="#" role="tab" tabindex="-1" aria-selected="false" aria-controls="tab-scoped-2">Employment History</a>
                                            </li>-->
                                            <li class="slds-tabs__item slds-text-heading--label slds-active " title="CVs" id="documentTab" role="presentation" style="display:{!IF(isclient , 'None', '')}">
                                                <a href="#" role="tab" tabindex="0" aria-selected="false" aria-controls="tab-scoped-1">CVs</a>
                                            </li>
                                          <!--  <li class="slds-tabs__item slds-text-heading--label" title="Upload Document" role="presentation">
                                                <a href="#" role="tab" tabindex="-1" aria-selected="false" aria-controls="tab-scoped-4">Upload Document</a>
                                            </li>-->
                                        </ul>   
                                        <div id="tab-scoped-1" class="slds-tabs__content slds-show" role="tabpanel" style="display:{!IF(isclient , 'None', '')}">
