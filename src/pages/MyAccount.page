                                                        <apex:commandButton styleClass="slds-button slds-button--brand" value="Save Profile" action="{!updateContactInfo}"  reRender="idHeader, idContactInfo" onComplete="updatePanelCss('{!isSuccess}', '{!strMessage}');" /> 
                                                    </apex:outputPanel>
                                                    
                                                    <apex:outputPanel rendered="{!!isEditable}" layout="none">
