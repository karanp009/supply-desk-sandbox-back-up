    <description>send a birthday text via sms360</description>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>/* The Date_of_birth__cis actual DOB so need to convert to the upcoming and 
     check if we&apos;ve missed it for this year, else schedule for next year */
 
IF ( 
      DATEVALUE( 
                               TEXT(YEAR(TODAY())) &amp; &quot;-&quot; &amp; 
                               TEXT(MONTH([Contact].Date_of_birth__c)) &amp; &quot;-&quot; &amp; 
                               TEXT(DAY([Contact].Date_of_birth__c))
                            ) &gt; TODAY(),
 
    /* Then schedule Birthday wish for this year 09:00 GMT = 9:00 MDT */
    DATETIMEVALUE( 
                                 TEXT(YEAR(TODAY())) &amp; &quot;-&quot; &amp; 
                                 TEXT(MONTH([Contact].Date_of_birth__c)) &amp; &quot;-&quot; &amp; 
                                 TEXT(DAY([Contact].Date_of_birth__c)) &amp; &quot; 09:00:00&quot; 
                               ),
 
    /* Else Add 1 Year and wish them Happy Birthday Next Year */
    DATETIMEVALUE( 
                                 TEXT(YEAR(TODAY())+1) &amp; &quot;-&quot; &amp; 
                                 TEXT(MONTH([Contact].Date_of_birth__c)) &amp; &quot;-&quot; &amp; 
                                 TEXT(DAY([Contact].Date_of_birth__c)) &amp; &quot; 09:00:00&quot; 
                               )
)
</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_2_myRule_1_A1_4864247425</name>
        <dataType>DateTime</dataType>
        <expression>/* The Date_of_birth__cis actual DOB so need to convert to the upcoming and 
     check if we&apos;ve missed it for this year, else schedule for next year */
 
