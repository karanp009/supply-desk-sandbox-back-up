                                                             <!--
                                                             <td class="slds-size--1-of-{!cntDocFields}">
                                                                <apex:inputField value="{!objContactDocument.TR1__Type__c}" styleClass="slds-input slds-m-bottom-medium" id="idDocumentType" /><br/>
                                                             </td>
                                                             -->
                                                             <td class="slds-size--1-of-{!cntDocFields}"></td>
                                                             
                                                        </tr>
                                                        <tr>
                                                            <center>
                                                                <td>  
                                                                <span id="idRequiredType" style="color:red;"></span>
                                                                <span id="idUploadDoc" style="color:red;"></span>
                                                                </td>
                                                            </center>
                                                        </tr>
                                                    </table>
                                                  </div>
                                            </div> 
                                            <!-- End New Code -->
                                            <!-- document listing block -->
                                            <apex:outputPanel id="Document">
                                                <apex:outputPanel rendered="{!lstDocument.size > 0}">
