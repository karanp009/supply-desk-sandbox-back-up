    <webLinks>
        <fullName>TR1__MassShortlist</fullName>
        <availability>online</availability>
        <description>Convert JobBoard Applications to Applications.</description>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Shortlist</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/33.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/33.0/apex.js&quot;)} 
var recordids = {!GETRECORDIDS($ObjectType.TR1__Job_Board_Application__c)}; 
if(recordids.length==0){ 
alert(&apos;Please select atleast one JobBoard Application to Convert.&apos;); 
}else{ 
var query = &quot;SELECT Id,Name, TR1__Applicant__c, TR1__Job__c, TR1__Job_Board_Source__c, TR1__Source__c, TR1__Status__c FROM TR1__Job_Board_Application__c Where Id IN (&apos;&quot;+recordids.toString().replace(/,/g , &quot;&apos;,&apos;&quot;)+&quot;&apos;)&quot;; 
try{ 
var selectedjobappln = sforce.connection.query(query); 
var selectedjobapplnrecords = selectedjobappln.getArray(&quot;records&quot;); 
if(selectedjobapplnrecords.length == 0){ 
alert(&apos;No Data found with selected Records.&apos;); 
location.reload(true); 
}else{ 
var newapplns = []; 
var isdelete = &quot;{!$Setup.TR1__TalentRoverProperties__c.TR1__Delete_JobBoard_Applications__c}&quot;; 
for(var i=0; i &lt; selectedjobapplnrecords.length ; i ++){ 
var newappln = new sforce.SObject(&quot;TR1__Application__c&quot;); 
newappln.TR1__Applicant__c=selectedjobapplnrecords[i].TR1__Applicant__c; 
newappln.TR1__Job__c=selectedjobapplnrecords[i].TR1__Job__c; 
newappln.TR1__Job_Board_Source__c=selectedjobapplnrecords[i].TR1__Job_Board_Source__c; 
newappln.TR1__Source__c=selectedjobapplnrecords[i].TR1__Source__c; 
newappln.TR1__Status__c=&apos;Current&apos;;
newapplns.push(newappln); 
} 
var result = sforce.connection.create(newapplns);
var updatejobapplns = [];
var deleteappids = [];
var errors =&apos;&apos;;
for(var j=0; j&lt;result.length; j++){
if(result[j].success==&apos;true&apos;){
deleteappids.push(selectedjobapplnrecords[j].Id);
var updatejobappln = new sforce.SObject(&quot;TR1__Job_Board_Application__c&quot;);
updatejobappln.Id = selectedjobapplnrecords[j].Id;
updatejobappln.TR1__Status__c =&apos;Converted&apos;; 
updatejobapplns.push(updatejobappln); 
errors+=&apos;Status of : &apos;+selectedjobapplnrecords[j].Id+&apos;  &apos;+selectedjobapplnrecords[j].Name+&apos; is : Success&apos;;
}else{
     errors+= &apos;Status of : &apos;+selectedjobapplnrecords[j].Id+&apos;  &apos;+selectedjobapplnrecords[j].Name+&apos;  is : Fail, and Error Message is : &apos;+result[j].errors.message;
}
}
if(isdelete==&apos;true&apos;){
   var result1 = sforce.connection.deleteIds(deleteappids); 
   location.reload(true); 
}else{
   var result2 = sforce.connection.update(updatejobapplns); 
   location.reload(true); 
}

} 
}catch(er){ 
alert(er); 
} 
}</url>
